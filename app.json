{
  "expo": {
    "name": "Ringer",
    "slug": "ringer-client-mobile",
    "version": "1.7.0",
    "owner": "lif_platforms",
    "orientation": "portrait",
    "icon": "./assets/global/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "ringer-mobile",
    "updates": {
      "enabled": false
    },
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/global/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#160900",
      "autoHide": false
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.lifplatforms.ringer",
      "jsEngine": "hermes",
      "icon": "./assets/global/ringer_icon_ios.icon"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/global/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lifplatforms.ringer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2873c89f-ddd2-4ec5-8f5b-2406277488d5"
      }
    },
    "plugins": [
      [
        "expo-build-properties"
      ],
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "ringer-mobile",
          "organization": "lif-platforms"
        }
      ],
      "expo-router",
      "expo-font"
    ]
  }
}
